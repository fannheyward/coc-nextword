{
  "name": "coc-nextword",
  "version": "0.3.0",
  "description": "nextword extension for coc.nvim",
  "author": "Heyward Fann <fannheyward@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "coc.nvim"
  ],
  "engines": {
    "coc": "^0.0.70"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fannheyward/coc-nextword"
  },
  "scripts": {
    "clean": "rimraf lib",
    "lint": "eslint src --ext ts",
    "watch": "webpack --watch",
    "build": "webpack",
    "prepare": "npm-run-all clean build"
  },
  "devDependencies": {
    "@types/node": "^13.9.3",
    "@types/which": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "coc.nvim": "^0.0.77",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.1",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "which": "^2.0.2"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 180,
    "semi": true
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "coc-nextword configuration",
      "properties": {
        "coc.source.nextword.enable": {
          "type": "boolean",
          "default": true
        },
        "coc.source.nextword.priority": {
          "type": "integer",
          "default": 90
        },
        "coc.source.nextword.shortcut": {
          "type": "string",
          "default": "Next"
        },
        "nextword.filetypes": {
          "type": "array",
          "default": [
            "markdown",
            "gitcommit"
          ],
          "description": "Enable nextword for these filetypes, `[\"*\"]` for all, and empty to disable"
        },
        "nextword.dataPath": {
          "type": "string",
          "default": "",
          "description": "Data path of nextword dataset. Use $NEXTWORD_DATA_PATH by default"
        },
        "nextword.count": {
          "type": "number",
          "default": 100,
          "description": "Max candidates word number"
        },
        "nextword.greedy": {
          "type": "boolean",
          "default": true,
          "description": "Show as many result as possible"
        },
        "nextword.wasm": {
          "type": "boolean",
          "default": false,
          "description": "Use bundled nextword.wasm"
        }
      }
    },
    "commands": []
  }
}
