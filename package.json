{
  "name": "coc-nextword",
  "version": "0.4.1",
  "description": "nextword extension for coc.nvim",
  "author": "Heyward Fann <fannheyward@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "coc.nvim"
  ],
  "engines": {
    "coc": "^0.0.80"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fannheyward/coc-nextword"
  },
  "scripts": {
    "build": "node esbuild.js",
    "prepare": "node esbuild.js"
  },
  "devDependencies": {
    "@types/node": "^14.0.6",
    "@types/which": "^2.0.0",
    "coc.nvim": "^0.0.80",
    "esbuild": "^0.8.54",
    "typescript": "^4.0.2",
    "which": "^2.0.2"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 180,
    "semi": true
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "coc-nextword configuration",
      "properties": {
        "coc.source.nextword.enable": {
          "type": "boolean",
          "default": true
        },
        "coc.source.nextword.priority": {
          "type": "integer",
          "default": 90
        },
        "coc.source.nextword.shortcut": {
          "type": "string",
          "default": "Next"
        },
        "nextword.filetypes": {
          "type": "array",
          "default": [
            "markdown",
            "gitcommit"
          ],
          "description": "Enable nextword for these filetypes, `[\"*\"]` for all, and empty to disable"
        },
        "nextword.dataPath": {
          "type": "string",
          "default": "",
          "description": "Data path of nextword dataset. Use $NEXTWORD_DATA_PATH by default"
        },
        "nextword.count": {
          "type": "number",
          "default": 100,
          "description": "Max candidates word number"
        },
        "nextword.greedy": {
          "type": "boolean",
          "default": true,
          "description": "Show as many result as possible"
        },
        "nextword.wasm": {
          "type": "boolean",
          "default": false,
          "description": "Use bundled nextword.wasm"
        }
      }
    },
    "commands": []
  }
}
